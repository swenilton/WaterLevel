<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<!--  	<display-name>jersey-tutorial</display-name> -->
	<servlet>
		<servlet-name>ServletUser</servlet-name>
		<servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer
		</servlet-class>
		<init-param>
			<param-name>com.sun.jersey.config.property.packages</param-name>
			<param-value>br.com.coffeebeans.usuario</param-value>
			
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletUser</servlet-name>
		<url-pattern>/WS/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ServletAtividade</servlet-name>
		<servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer
		</servlet-class>
		<init-param>
			<param-name>com.sun.jersey.config.property.packages</param-name>
			<param-value>br.com.coffeebeans.atividade</param-value>
			
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletAtividade</servlet-name>
		<url-pattern>/WS2/*</url-pattern>
	</servlet-mapping>
</web-app> 
